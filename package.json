{
	"name": "auto_linker",
	"version": "1.0.0",
	"description": "Automatically creates links to pages and headers in your notes as you type.",
	"main": "main.js",
	"scripts": {
		"start": "node esbuild.config.mjs",
		"dev": "tsc --noEmit --skipLibCheck && npm run start",
		"build": "tsc --noEmit --skipLibCheck && node lint/esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint . --ext .ts"
	},
	"keywords": [
		"obsidian-plugin",
		"note-tracker",
		"typescript",
		"productivity"
	],
	"author": "Tom Chambers",
	"license": "",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^8.0.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"date-fns": "^3.6.0",
		"marked": "^14.1.1"
	}
}
